function add(arr)
{
    var sum=0;
    for(let i=4;i<arr.length;i++)
    {
        sum+=parseInt(arr[i]);
    }
    return sum;
}
function mul(arr)
{
    var tot=1;
    for(let i=4;i<arr.length;i++)
    {
        tot*=parseInt(arr[i]);
    }
    return tot;
}
function sub(arr)
{
    return parseInt(arr[4])-parseInt(arr[5]);
}
function div(arr)
{
    return parseInt(arr[4])/parseInt(arr[5]);
}
